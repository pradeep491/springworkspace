<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="emp" class="com.test.Employee">
		<property name="id" value="491" />
		<property name="name" value="Pradeep Kumar K" />
		<property name="Address">
			<ref bean="address" />
		</property>
	</bean>
	<bean id="address" class="com.test.Address">
		<property name="city" value="Nandyala" />
		<property name="state" value="Andhra Pradesh" />
		<property name="country" value="India" />
	</bean>
</beans>  